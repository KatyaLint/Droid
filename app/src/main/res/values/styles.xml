<resources>
    <!--in values-v21-->
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor">@color/COLOR_003D4C</item>
    </style>

    <style name="AppTheme.ActionBar.Transparent" parent="AppTheme">

    </style>

    <style name="AppTheme.ActionBar" parent="AppTheme">
        <item name="windowActionBarOverlay">false</item>
        <item name="colorPrimary">@color/COLOR_003D4C</item>
    </style>

    <style name="DrawerArrowStyle" parent="Widget.AppCompat.DrawerArrowToggle">

        <item name="color">@android:color/white</item>
    </style>
    <style name="bottom_bar_text">
        <item name="android:textSize">@dimen/SP10</item>
        <item name="android:fontFamily">dinnextltpro_regular.otf</item>
    </style>

    <style name="NavigationTextStyle">
        <item name="android:textColor">@color/navigation_text_state</item>
        <item name="android:textSize">@dimen/SP18</item>
        <item name="android:fontFamily">dinnextltpro_regular.otf</item>
    </style>


    <style name="StateText">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:padding">4dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:background">#33000000</item>
    </style>


    <style name="GridTextStyle">

        <!--<item name="android:paddingLeft">@dimen/DP4</item>-->
        <item name="android:layout_marginBottom">@dimen/DP4</item>
        <item name="android:maxLines">1</item>
    </style>

    <style name="register_edit_text">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/SP20</item>
        <item name="android:textColorHint">@color/COLOR_999999</item>
        <item name="android:textColor">@color/COLOR_WHITE</item>
        <item name="android:background">@drawable/edit_text_border</item>

    </style>

    <style name="cc_edit_text_style">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textCursorDrawable">@null</item>
        <item name="android:textSize">@dimen/SP16</item>
        <item name="android:paddingBottom">@dimen/DP10</item>
        <item name="android:textColorHint">@color/COLOR_999999</item>
        <item name="android:textColor">@color/COLOR_BLACK</item>
        <item name="android:background">@drawable/cc_edit_text_border</item>
    </style>

    <style name="cc_number_edit_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textCursorDrawable">@null</item>
        <item name="android:textSize">@dimen/SP16</item>
        <item name="android:paddingBottom">@dimen/DP10</item>
        <item name="android:textColorHint">@color/COLOR_565656</item>
        <item name="android:textColor">@color/COLOR_BLACK</item>
    </style>

    <style name="CustomButtonTextStyle">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fontName">dinnextltpro_medium.otf</item>
        <item name="android:textSize">@dimen/SP18</item>
        <item name="android:paddingBottom">@dimen/DP10</item>
        <item name="android:paddingTop">@dimen/DP10</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/COLOR_WHITE</item>
    </style>


    <style name="RedCustomButtonStyle" parent="CustomButtonTextStyle">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:background">@drawable/selector_red_button</item>
    </style>


    <style name="CustomButtonTextStyleNew">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fontName">dinnextltpro_medium.otf</item>
        <item name="android:textSize">@dimen/SP18</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">@dimen/DP10</item>
        <item name="android:paddingTop">@dimen/DP10</item>
        <item name="android:textColor">@color/COLOR_WHITE</item>
        <item name="android:layout_marginLeft">@dimen/DP20</item>
        <item name="android:layout_marginRight">@dimen/DP20</item>

    </style>

    <style name="RedButton" parent="CustomButtonTextStyleNew">
        <item name="android:textColor">@color/COLOR_EE3A3C</item>
        <item name="android:background">@drawable/red_button_border</item>
    </style>

    <style name="BlueButton" parent="CustomButtonTextStyleNew">
        <item name="android:textColor">@color/COLOR_003D4C</item>
        <item name="android:background">@drawable/blue_button_border</item>
    </style>

    <style name="CountryButton" >
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="fontName">dinnextltpro_medium.otf</item>
        <item name="android:textSize">@dimen/SP13</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/COLOR_999999</item>
        <item name="android:background">@drawable/blue_button_border</item>
    </style>


    <style name="RedCustomButtonStyleNoRadius" parent="CustomButtonTextStyle">
        <item name="android:textColor">@color/COLOR_WHITE</item>
        <item name="android:background">@drawable/selector_red_button_no_radius</item>
    </style>

  <!--  <style name="RedCustomButtonDisableStyle" parent="CustomButtonTextStyle">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:background">@drawable/red_disable_button</item>
    </style>-->

