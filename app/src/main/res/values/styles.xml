        <item name="android:button">@drawable/custom_radio_button</item>
    </style>

    <style name="MyRadioCheckButtonStyle" parent="@android:style/Widget.CompoundButton.RadioButton">
        <item name="android:button">@drawable/custom_radio_check_button</item>
    </style>-->

    <style name="RedCheckoutCustomButtonStyle" parent="CustomButtonTextStyle">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">@dimen/DP16</item>
        <item name="android:textSize">@dimen/SP16</item>

        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/red_button_no_radius</item>
    </style>
    <style name="RedCheckoutCustomButtonDisableStyle" parent="CustomButtonTextStyle">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">@dimen/DP16</item>
        <item name="android:textSize">@dimen/SP16</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/red_disable_button_no_radius</item>
    </style>

    <style name="GrayCustomButtonStyle" parent="CustomButtonTextStyle">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:background">@drawable/rounded_corner_opacity</item>
    </style>

    <style name="CNCStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxWidth">@dimen/DP300</item>
        <item name="android:minWidth">@dimen/DP80</item>
        <item name="android:textSize">@dimen/SP18</item>
        <item name="android:padding">@dimen/DP10</item>

    </style>

    <style name="TextViewTravlerCheckoutDetails">
        <item name="android:textSize">@dimen/SP14</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/COLOR_565656</item>



    </style>

    <style name="PaymentItemStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:maxWidth">@dimen/DP300</item>
        <item name="android:minWidth">@dimen/DP50</item>
        <item name="android:textColor">@color/COLOR_565656</item>
        <item name="android:padding">@dimen/DP2</item>


    </style>


    <style name="PaymentCheckBoxStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:checked">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:button">@drawable/selector_hgb_checkbox</item>
    </style>






    <style name="PaymentSelectCCStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/SP10</item>
        <item name="android:text">Select Card</item>
        <item name="android:textColor">@color/COLOR_00516f</item>
        <item name="android:padding">@dimen/DP16</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:background">@drawable/select_cc_border</item>
        <item name="android:drawableRight">@drawable/expand_more_a_1</item>
    </style>

    <style name="NewPaymentSelectCCStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/SP10</item>
        <item name="android:text">Select Card</item>
        <item name="android:textColor">@color/COLOR_00516f</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <style name="TravelerDetailsTextStyle">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/SP18</item>
        <item name="android:inputType">textNoSuggestions</item>
        <item name="android:padding">@dimen/DP10</item>
    </style>

    <style name="GridViewPassangersTextStyle">
        <item name="android:textSize">@dimen/SP16</item>
        <item name="android:fontFamily">dinnextltpro_regular.otf</item>
        <item name="android:textColor">@color/COLOR_WHITE</item>
        <item name="android:gravity">center</item>
        <item name="android:weightSum">1</item>
    </style>

    <style name="PersonalInformationEditText">
        <item name="android:textSize">@dimen/SP16</item>
        <item name="android:textColor">@color/COLOR_003D4C</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_marginTop">@dimen/DP8</item>
        <item name="android:layout_marginBottom">@dimen/DP8</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


    <style name="Sign_up_text_color">
        <item name="android:textColor">@color/COLOR_WHITE</item>
        <item name="android:textColorHint">@color/COLOR_00516f</item>

        <item name="android:textSize">@dimen/SP16</item>
        <item name="android:textColorHighlight">@color/COLOR_WHITE</item>
        <item name="fontName">dinnextltpro_regular.otf</item>
        <item name="android:padding">@dimen/DP10</item>
        <item name="android:layout_margin">@dimen/DP10</item>
    </style>

    <style name="new_sign_up_text_color">
        <item name="android:textColor">@color/COLOR_BLACK</item>
        <item name="android:layout_width">@dimen/DP340</item>
        <item name="android:textColorHint">@color/COLOR_999999</item>
        <item name="android:textSize">@dimen/SP16</item>
        <item name="android:textColorHighlight">@color/COLOR_999999</item>
        <item name="fontName">dinnextltpro_regular.otf</item>

    </style>

    <style name="adress_edit_text">
        <item name="android:textColor">@color/COLOR_BLACK</item>
        <item name="android:textColorHint">@color/COLOR_999999</item>
        <item name="android:textSize">@dimen/SP16</item>
        <item name="android:textColorHighlight">@color/COLOR_999999</item>
        <item name="fontName">dinnextltpro_regular.otf</item>
    </style>

    <style name="TutAppTheme" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>


    <style name="flight_details_view_title">
        <item name="android:textSize">@dimen/SP13</item>
        <item name="android:textColor">@color/COLOR_565656</item>
        <item name="fontName">dinnextltpro_medium.otf</item>
    </style>

    <style name="flight_details_view_text">
        <item name="android:textSize">@dimen/SP13</item>
        <item name="android:textColor">@color/COLOR_565656</item>
        <item name="fontName">dinnextltpro_regular.otf</item>
    </style>


    <style name="alternative_hotel_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/COLOR_565656</item>
        <item name="android:textSize">@dimen/SP13</item>
        <item name="fontName">dinnextltpro_medium.otf</item>
    </style>


    <style name="alternative_hotel_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/COLOR_565656</item>
        <item name="android:textSize">@dimen/SP14</item>
        <item name="fontName">dinnextltpro_regular.otf</item>
    </style>

    <style name="Divider">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>

        <item name="android:background">@android:color/darker_gray</item>
    </style>

</resources>
