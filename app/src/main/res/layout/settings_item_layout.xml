<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:padding="@dimen/DP10"
    android:id="@+id/settings_ll"
    >


    <hellogbye.com.hellogbyeandroid.views.FontTextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/SP16"
        android:textColor="@color/profile_blue"
        android:id="@+id/preferenses_name"
        app:fontName="dinnextltpro_regular.otf"/>


    <include android:id="@+id/preferences_more" layout="@layout/layout_arrow"
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
    />

</RelativeLayout>
