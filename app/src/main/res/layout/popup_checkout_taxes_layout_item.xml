<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_weight="2">

    <hellogbye.com.hellogbyeandroid.views.FontTextView

        android:layout_alignParentLeft="true"
        android:id="@+id/check_out_taxes_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@+id/image_profile_switch_a_1"
        android:textColor="@color/COLOR_565656"
        android:textSize="@dimen/SP16"

        app:fontName="dinnextltpro_regular.otf"
        android:padding="@dimen/DP20"
        android:text="@string/profile_choose_between"
        android:layout_toLeftOf="@+id/check_out_taxes_price"
        android:layout_toStartOf="@+id/check_out_taxes_price" />



    <hellogbye.com.hellogbyeandroid.views.FontTextView
        android:layout_alignParentRight="true"
        android:id="@+id/check_out_taxes_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/COLOR_565656"
        android:textSize="@dimen/SP16"
        app:fontName="dinnextltpro_medium.otf"
        android:padding="@dimen/DP20"
        android:text="@string/profile_choose_between"/>


    <!-- profile_choose_between-->


</RelativeLayout>



