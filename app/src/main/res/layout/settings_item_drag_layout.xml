<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/DP10">


    <hellogbye.com.hellogbyeandroid.views.FontTextView
            android:id="@+id/settings_place_number"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="@color/dark_teal"
            android:textSize="@dimen/SP20"
            android:text="10"
            android:layout_centerVertical="true"
            android:layout_marginEnd="@dimen/DP10"
            android:layout_marginLeft="@dimen/DP10"
            app:fontName="dinnextltpro_condensed.ttf"/>

    <hellogbye.com.hellogbyeandroid.views.FontTextView
        android:layout_centerVertical="true"
        android:id="@+id/setting_text_drag"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/SP14"
        android:text="dfsdfsdf"
        android:layout_toRightOf="@+id/settings_place_number"
        app:fontName="dinnextltpro_regular.otf"
        android:textColor="@color/COLOR_555555"
        android:lines="1"/>

</RelativeLayout>
