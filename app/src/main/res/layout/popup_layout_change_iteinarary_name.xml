<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout_root" android:orientation="vertical"
    android:layout_width="fill_parent" android:layout_height="fill_parent"
    android:paddingLeft="@dimen/DP20" android:paddingRight="@dimen/DP20"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingBottom="@dimen/DP20" android:paddingTop="@dimen/DP14" android:layout_gravity="top">


    <hellogbye.com.hellogbyeandroid.views.FontTextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/edit_trip_name"
        android:textColor="@color/COLOR_00516f"
        app:fontName="dinnextltpro_condensed.ttf"
        android:id="@+id/title_text_change_name"
        android:textSize="@dimen/SP24"/>



    <hellogbye.com.hellogbyeandroid.views.FontEditTextView
        android:id="@+id/change_iteinarary_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:hint=""
        android:textColorHint="@color/COLOR_80FFFFFF"/>

</LinearLayout>
